{
    "name": "Gameplay Automator_Beta_v001",
    "version": "0.13.01",
    "manifest_version": 3,
    "description": "Gameplay Automator",
    "action": {
        "default_icon": "33.png",
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": ["https://game.league17.ru/*", "https://league17.ru/*"],
            "js": ["jquery-3.6.0.js", "jscrypt.js", "happy_birthday_test.js"],
            "run_at": "document_idle"
         }
    ]
}
